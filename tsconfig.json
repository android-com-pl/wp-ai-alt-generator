{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "typeRoots": ["node_modules/@types", "src/types"]
  },
  "include": ["src"],
  "exclude": ["build", "node_modules", "vendor"]
}
